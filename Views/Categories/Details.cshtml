@using bookStore.ViewModel
model CategoryVM
@section Title {
    Categories
}
@section subTitle {
    Category Details
}
<div class="card">
    <div class="card-body">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th style="width: 10px">Id</th>
                    <th>Category Name</th>
                    <th>Created At</th>
                    <th>Updated At</th>
                </tr>
            </thead>
            <tbody>
                    <tr>
                        <td>@Model.Id</td>
                    <td>@Model.Name</td>
                    <td>@Model.CreatedAt</td>
                    <td>@Model.UpdatedAt</td>
                    </tr>
            </tbody>
        </table>
    </div>
</div>